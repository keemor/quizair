{
    "name": "quizair",
    "version": "1.0.0",
    "description": "Node Express Mongo Parcel Preact PWA hosted on herokuapp.com",
    "scripts": {
        "start": "node server/server.js",
        "mon": "nodemon server/server.js",
        "parcel:watch": "node node_modules/parcel-bundler/bin/cli.js watch client/index.html",
        "parcel:build": "node node_modules/parcel-bundler/bin/cli.js build client/index.html",
        "test": "mocha  --exit server/**/*.test.js",
        "test:watch": "nodemon --exec 'npm test'"
    },
    "author": "keemor",
    "license": "ISC",
    "dependencies": {
        "babel": "^6.23.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-runtime": "^6.26.0",
        "body-parser": "^1.18.3",
        "compression": "^1.7.3",
        "express": "^4.16.3",
        "express-sslify": "^1.2.0",
        "history": "^4.7.2",
        "mongodb": "^3.1.3",
        "mongoose": "^5.2.8",
        "parcel-bundler": "^1.9.7",
        "parcel-plugin-sw-cache": "^0.2.5",
        "preact": "8.2.9",
        "preact-material-components": "1.5.0",
        "preact-router": "^2.6.1",
        "unistore": "^3.0.6"
    },
    "devDependencies": {
        "expect": "^23.5.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.3",
        "supertest": "^3.1.0"
    }
}
